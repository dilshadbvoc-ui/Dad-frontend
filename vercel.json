{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/shared-product/:slug",
            "destination": "/index.html"
        },
        {
            "source": "/((?!api|_next|static|favicon.ico|assets|.*\\..*).*)",
            "destination": "/index.html"
        }
    ]
}